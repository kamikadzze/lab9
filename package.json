    "ci": "npm run lint && npm test"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "eslint": "^8.57.1",
    "globals": "^15.13.0",
    "jest": "^29.7.0",
    "typescript-eslint": "^8.18.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
